{
  "manifest_version": 3,
  "permissions": [
    "scripting",
    "activeTab"
  ],
    "name": "MAP",
    "description": "MAP",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "*://*/*"
    ],
    "web_accessible_resources": [
      {
        "resources": ["contentScript.js"],
        "matches": ["<all_urls>"]
      }
    ]
  }

